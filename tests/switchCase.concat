test proc int => string :
  #_input _# switch
  1 case
    "1st case" return
    break
  2 case
    "2nd case" return
    break
  3 case
    "3rd case" return
    break
  0 4 5 6 7 8 9 case
    "0,4,5,6,7,8,9 case" return
    break
  end
  "fallthrough" return
end
testDefault proc int => string :
  9 swap -
  #_input _# switch
  1 case
    "1st case" return
    break
  2 case
